f0 = (x) ->
  assertTrue this is global
  x
g0 = (x, y) ->
  f0 x is y
f1 = (x) ->
  assertTrue this is o
  x
g1 = (x, y) ->
  o.f1 x is y
global = this
assertTrue g0(0, 0)
assertFalse g0(0, 1)
o = {}
o.f1 = f1
assertTrue g1(0, 0)
assertFalse g1(0, 1)
