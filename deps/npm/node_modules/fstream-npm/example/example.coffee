# this will show what ends up in the fstream-npm package
P = require("../")
P(path: "./").on("package", (p) ->
  console.error "package", p
  return
).on("ignoreFile", (e) ->
  console.error "ignoreFile", e
  return
).on "entry", (e) ->
  console.error e.constructor.name, e.path.substr(e.root.dirname.length + 1)
  return

