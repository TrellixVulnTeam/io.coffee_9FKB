#test that parse(stringify(obj) deepEqu
ini = require("../")
test = require("tap").test
data =
  number:
    count: 10

  string:
    drink: "white russian"

  boolean:
    isTrue: true

  "nested boolean":
    theDude:
      abides: true
      rugCount: 1

test "parse(stringify(x)) deepEqual x", (t) ->
  for k of data
    s = ini.stringify(data[k])
    console.log s, data[k]
    t.deepEqual ini.parse(s), data[k]
  t.end()
  return

