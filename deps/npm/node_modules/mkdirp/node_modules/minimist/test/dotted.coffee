parse = require("../")
test = require("tape")
test "dotted alias", (t) ->
  argv = parse([
    "--a.b"
    "22"
  ],
    default:
      "a.b": 11

    alias:
      "a.b": "aa.bb"
  )
  t.equal argv.a.b, 22
  t.equal argv.aa.bb, 22
  t.end()
  return

test "dotted default", (t) ->
  argv = parse("",
    default:
      "a.b": 11

    alias:
      "a.b": "aa.bb"
  )
  t.equal argv.a.b, 11
  t.equal argv.aa.bb, 11
  t.end()
  return

