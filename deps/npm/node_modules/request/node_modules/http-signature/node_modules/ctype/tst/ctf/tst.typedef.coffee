test = ->
  data = undefined
  parser = undefined
  data = JSON.parse(mod_fs.readFileSync("./typedef.json").toString())
  parser = mod_ctype.parseCTF(data,
    endian: "big"
  )
  mod_assert.deepEqual parser.lstypes(),
    bar_t: "int"
    int: "int32_t"

  return
mod_fs = require("fs")
mod_ctype = require("../../ctype.js")
mod_assert = require("assert")
test()
