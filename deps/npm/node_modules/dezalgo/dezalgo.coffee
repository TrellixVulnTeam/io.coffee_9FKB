dezalgo = (cb) ->
  sync = true
  asap ->
    sync = false
    return

  zalgoSafe = ->
    args = arguments
    me = this
    if sync
      asap ->
        cb.apply me, args
        return

    else
      cb.apply me, args
    return
wrappy = require("wrappy")
module.exports = wrappy(dezalgo)
asap = require("asap")
