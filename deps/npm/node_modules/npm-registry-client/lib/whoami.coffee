whoami = (uri, cb) ->
  return cb(new Error("Must be logged in to see who you are"))  unless @conf.getCredentialsByURI(uri)
  @request "GET", url.resolve(uri, "whoami"), null, (er, userdata) ->
    return cb(er)  if er
    cb null, userdata.username
    return

  return
module.exports = whoami
url = require("url")
