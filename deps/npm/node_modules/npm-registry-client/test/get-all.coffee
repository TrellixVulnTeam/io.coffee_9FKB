tap = require("tap")
server = require("./lib/server.js")
common = require("./lib/common.js")
client = common.freshClient()
tap.test "basic request", (t) ->
  server.expect "/-/all", (req, res) ->
    res.json []
    return

  client.get "http://localhost:1337/-/all", null, (er) ->
    t.ifError er, "no error"
    t.end()
    return

  return

