# clean up the fixtures
tap = require("tap")
rimraf = require("rimraf")
test = tap.test
path = require("path")
test "clean fixtures", (t) ->
  rimraf path.resolve(__dirname, "fixtures"), (er) ->
    t.ifError er, "rimraf ./fixtures/"
    t.end()
    return

  return

test "clean tmp", (t) ->
  rimraf path.resolve(__dirname, "tmp"), (er) ->
    t.ifError er, "rimraf ./tmp/"
    t.end()
    return

  return

