test = (c) ->
  assert c.constructor is Child
  assert c.constructor.super_ is Parent
  assert Object.getPrototypeOf(c) is Child::
  assert Object.getPrototypeOf(Object.getPrototypeOf(c)) is Parent::
  assert c instanceof Child
  assert c instanceof Parent
  return
Child = ->
  Parent.call this
  test this
  return
Parent = ->
inherits = require("./inherits.js")
assert = require("assert")
inherits Child, Parent
c = new Child
test c
console.log "ok"
