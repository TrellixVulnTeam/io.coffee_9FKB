readInstalled = require("../read-installed.js")
test = require("tap").test
path = require("path")
test "\"latest\" version is valid", (t) ->
  
  # This test verifies npm#3860
  readInstalled path.join(__dirname, "fixtures/peer-at-latest"),
    log: console.error
  , (err, map) ->
    t.notOk map.dependencies.debug.invalid, "debug@latest is satisfied by a peer"
    t.end()
    return

  return

