sigmund = (subject, maxSessions) ->
  psychoAnalyze = (subject, session) ->
    return  if session > maxSessions
    return  if typeof subject is "function" or typeof subject is "undefined"
    if typeof subject isnt "object" or not subject or (subject instanceof RE)
      analysis += subject
      return
    return  if notes.indexOf(subject) isnt -1 or session is maxSessions
    notes.push subject
    analysis += "{"
    Object.keys(subject).forEach (issue, _, __) ->
      
      # pseudo-private values.  skip those.
      return  if issue.charAt(0) is "_"
      to = typeof subject[issue]
      return  if to is "function" or to is "undefined"
      analysis += issue
      psychoAnalyze subject[issue], session + 1
      return

    return
  maxSessions = maxSessions or 10
  notes = []
  analysis = ""
  RE = RegExp
  psychoAnalyze subject, 0
  analysis
module.exports = sigmund

